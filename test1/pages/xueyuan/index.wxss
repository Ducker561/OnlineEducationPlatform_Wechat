.main {
    font-size: 32rpx;
    font-family: 'Microsoft YaHei';
    color: #555
}

.imagearea {
    width: 100%;
    padding: 8px 0;
    height: 390rpx;
    overflow: hidden;
}

.imagearea image {
    width: 100%;
    display: block;
}

.text {
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
}

.inner {
    width: 96%;
    margin: 20rpx auto;
}

.ubom10rpx {
    clear: both;
    height: 10rpx;
    background: #eee;
}


.strengthlist {
    width: 96%;
    height: 190rpx;
    padding: 2%;
    border-bottom: 1px solid #eee
}

.strengthimg {
    float: left;
    width: 30%;
    height: 100%;
}

.strengthimg image {
    float: left;
    width: 100%;
    height: 100%;
}

.strengthinfo {
    float: left;
    width: 66%;
    text-align: left;
    line-height: 60rpx;
    margin-left: 3%
}

.strengthinfo .ilist {
    clear: both;
}

.strengthinfo .zhicheng {
    margin-left: 20rpx;
    background: #888;
    border-radius: 5rpx;
    padding: 5rpx;
    color: #fff
}

.utitle {
    width: 100%;
    height: 90rpx;
    text-align: center;
    background: #fff;
}

.mtitle {
    font-size: 35rpx;
    font-weight: bold;
    color: #000;
    margin-top: 10rpx;
}

.ztitle {
    font-size: 30rpx;
    color: #999
}