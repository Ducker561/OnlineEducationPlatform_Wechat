.head {
  padding-top: 30px;
  text-align: center;
  font-weight: bold;
  font-size: 20px;
}

.table {
  display: inline-flex;
  flex-direction: column;
  border: 1px solid rgba(218, 217, 217, 1);
  border-bottom: 0;
}

.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: #fff;
}

.table_header {
  display: inline-flex;
  background: #e8e8e8;
}

.bg_g{
  background: #f8f8f8;
}

.cell_label {
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(74, 74, 74, 1);
}

.data {
  display: flex;
  flex-direction: row;
}



.th {
  display: flex;
  width: 200rpx;
